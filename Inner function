var x=prompt();
var y=prompt();
function sumOfFact(x)
{
   if(x==0)
   {
       return 1;
   } 
   return x*sumOfFact(x-1);
}
console.log(sumOfFact(x)+sumOfFact(y));

function example()
{
    return{
        name:"Rabby",  // function as an object//
        Roll:"ASh1901034M",
        print:function()
        {
            console.log(this.name,this.roll);
        }


    }
    
}
console.log(example());

function returN()
{
    return{
       name:'Yasin',
       CV: ['Roll:Ash1901034M','Department:CSTE'],
       print:function()
       {
           console.log(this.name,this.CV);
       }
    }
}
var res=returN();
console.log(res);
var rs=10+(function(){return 20})();
console.log(rs);






