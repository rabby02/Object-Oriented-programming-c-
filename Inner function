function outer()
{
    function inner()
    {
        console.log("Hello");
    }
    inner();
    console.log("Hi");
}
outer();

function sumAtion()
{
     var sum=0;
    function addiTion()
    {
       
        for(var i=1;i<10;i++)
        {
            sum=sum+i;
        }
        console.log(sum);

    }
       
      addiTion();
    console.log("Algebraic expression");
}
sumAtion();


